/*************  ✨ Windsurf Command ⭐  *************/
<script>
    const urlParams = new URLSearchParams(window.location.search);
    const query = urlParams.get('query');
    if (query) {
        document.getElementById('autocomplete-input').value = query;
    document.getElementById('autocomplete-input').dispatchEvent(new Event('input'));
        }
</script>
